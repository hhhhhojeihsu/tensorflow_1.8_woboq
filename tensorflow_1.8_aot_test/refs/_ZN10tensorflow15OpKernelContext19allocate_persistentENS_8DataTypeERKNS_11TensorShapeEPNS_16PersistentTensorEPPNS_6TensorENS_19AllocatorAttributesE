<dec f='tensorflow/tensorflow/core/framework/op_kernel.h' l='880' type='tensorflow::Status tensorflow::OpKernelContext::allocate_persistent(tensorflow::DataType type, const tensorflow::TensorShape &amp; shape, tensorflow::PersistentTensor * out_persistent, tensorflow::Tensor ** out_tensor, tensorflow::AllocatorAttributes attr)'/>
<use f='tensorflow/tensorflow/core/framework/op_kernel.h' l='886' u='c' c='_ZN10tensorflow15OpKernelContext19allocate_persistentENS_8DataTypeERKNS_11TensorShapeEPNS_16PersistentTensorEPPNS_6TensorE'/>
<doc f='tensorflow/tensorflow/core/framework/op_kernel.h' l='872'>// Allocates a Tensor of the specified type and shape which the Op
  // plans to maintain as persistent state. out_persistent holds the
  // PersistentTensor which is the object the caller should store. For
  // convenience, if out_tensor is non-null then it will be filled in
  // with a Tensor* pointing to the newly-allocated tensor which the
  // caller can use instead of calling
  // out_persistent-&gt;AccessTensor. The caller does not own out_tensor
  // and should not keep a copy of it. See comment above.</doc>
